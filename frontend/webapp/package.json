{
  "name": "voicetree-webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:test": "vite build",
    "lint": "eslint .",
    "check": "eslint . && tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest --no-isolate run && playwright test tests/e2e/full-electron/electron-real-folder.spec.ts --config=playwright-electron.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:headless": "xvfb-run -a playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:electron": "electron-vite build --logLevel silent && playwright test --config=playwright-electron.config.ts",
    "test:browser": "playwright test tests/playwright-browser/isolated-with-harness/file-to-graph-pipeline-behavioral.spec.ts",
    "electron": "npx kill-port 3000 2>/dev/null || true && electron-vite dev --watch",
    "electron:prod": "electron-vite build && electron .",
    "electron:build": "electron-vite build",
    "electron:pack": "electron-vite build && electron-builder",
    "electron:dist": "electron-vite build && electron-builder --publish=never"
  },
  "dependencies": {
    "@codemirror/commands": "^6.10.0",
    "@codemirror/lang-java": "^6.0.2",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language-data": "^6.5.2",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.6",
    "@markdoc/markdoc": "^0.5.4",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@soniox/speech-to-text-web": "^1.2.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/cytoscape": "^3.21.9",
    "@types/lodash.debounce": "^4.0.9",
    "@xterm/addon-clipboard": "^0.1.0-beta.135",
    "@xterm/addon-fit": "^0.11.0-beta.135",
    "@xterm/addon-search": "^0.16.0-beta.135",
    "@xterm/addon-unicode11": "^0.9.0-beta.135",
    "@xterm/addon-webgl": "^0.19.0-beta.135",
    "@xterm/xterm": "^5.6.0-beta.135",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "codemirror": "^6.0.2",
    "codemirror-rich-markdoc": "^0.0.2",
    "cytoscape": "^3.33.1",
    "cytoscape-cola": "^2.5.1",
    "cytoscape-cxtmenu": "^3.5.0",
    "fix-path": "^5.0.0",
    "fp-ts": "^2.16.11",
    "gray-matter": "^4.0.3",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.544.0",
    "mermaid": "^11.12.0",
    "ninja-keys": "^1.2.2",
    "node-pty": "^1.0.0",
    "re-resizable": "^6.11.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-draggable": "^4.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.55.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/ui": "^3.2.4",
    "chokidar": "^4.0.3",
    "concurrently": "^9.1.0",
    "electron": "^38.1.2",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.1",
    "eslint": "^9.30.1",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^27.0.0",
    "msw": "^2.11.3",
    "playwright": "^1.55.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.1"
  },
  "build": {
    "appId": "com.voicetree.webapp",
    "productName": "VoiceTree",
    "directories": {
      "output": "../../dist/electron"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "package.json",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "../../dist/resources/server",
        "to": "server"
      },
      {
        "from": "../../dist/resources/tools",
        "to": "tools"
      },
      {
        "from": "../../dist/resources/backend",
        "to": "backend"
      }
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
