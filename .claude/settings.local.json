{
  "permissions": {
    "allow": []
  },
  "hooks": {
    "PostToolUse": [
      {
        "matcher": "Write|Edit|MultiEdit",
        "hooks": [
          {
            "type": "command",
            "command": "node \"$CLAUDE_PROJECT_DIR/webapp/.claude/hooks/collect-changed-files.cjs\""
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node \"$CLAUDE_PROJECT_DIR/webapp/.claude/hooks/stop-quality-check.cjs\""
          }
        ]
      }
    ]
  },
  "enabledPlugins": {},
  "extraKnownMarketplaces": {
    "voicetree-plugins": {
      "source": {
        "source": "directory",
        "path": "./.claude/plugins"
      }
    }
  },
  "outputStyle": "Explanatory",
  "spinnerTipsEnabled": true,
  "prefersReducedMotion": false
}
